<?xml version="1.0"?>
<robot name="planar_manipulator_3dof">

	<link name="world" />
	
	<joint name="fixed_world" type="fixed">
	  <parent link="world" />
	  <child link="base_link" />
	  <origin rpy="0 0 0" xyz="-7 0 0"/>
	</joint>

	<link name="base_link">
	  <visual>
	    <geometry>
	      <cylinder length="1" radius="0.5" />
	    </geometry>
	    <origin rpy="0 0 0" xyz="0 0 0" />
	    <material name="blue">
	      <color rgba="0 0 1.0 1.0"/>
	    </material>
	  </visual>
	</link>

	<joint name="joint_0" type="revolute">
	  <parent link="base_link" />
	  <child link="link_0" />
	  <origin xyz="0 0 0.0" rpy="0 0 0" />
	  <axis xyz="0 0 1" />
	  <limit effort="100" velocity="20" />
	</joint>

	<link name="link_0">
	  <visual>
	    <origin xyz="5 0 0" rpy="0 0 0" />
	    <geometry>
	      <box size="10.0 0.2 0.1" />
	    </geometry>
	    <material name="blue"/>
	  </visual>
	  <inertial>
	    <origin xyz="5 0 0" rpy="0 0 0" />
	    <mass value="0.5" />
	    <inertia ixx="16.666666666666668" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="16.666666666666668" />
	  </inertial>
	</link>
	
	<joint name="joint_1" type="revolute">
	  <parent link="link_0" />
	  <child link="link_1" />
	  <origin xyz="10.0 0 0" rpy="0 0 0" />
	  <axis xyz="0 0 1" />
	  <limit effort="100" velocity="20" />
	</joint>

	<link name="link_1">
	  <visual>
	    <origin xyz="5 0 0" rpy="0 0 0" />
	    <geometry>
	      <box size="10.0 0.2 0.1" />
	    </geometry>
	    <material name="blue"/>
	  </visual>
	  <inertial>
	    <origin xyz="5 0 0" rpy="0 0 0" />
	    <mass value="0.5" />
	    <inertia ixx="16.666666666666668" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="16.666666666666668" />
	  </inertial>
	</link>
	
	<joint name="joint_2" type="revolute">
	  <parent link="link_1" />
	  <child link="link_2" />
	  <origin xyz="10.0 0 0" rpy="0 0 0" />
	  <axis xyz="0 0 1" />
	  <limit effort="100" velocity="20" />
	</joint>


	<link name="link_2">
	  <visual>
	    <origin xyz="5 0 0" rpy="0 0 0" />
	    <geometry>
	      <box size="10.0 0.2 0.1" />
	    </geometry>
	    <material name="blue"/>
	  </visual>
	  <inertial>
	    <origin xyz="5 0 0" rpy="0 0 0" />
	    <mass value="0.5" />
	    <inertia ixx="16.666666666666668" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="16.666666666666668" />
	  </inertial>
	</link>

       <joint name="fixed_ee" type="fixed">
	  <parent link="link_2" />
	  <child link="EE" />
	  <origin xyz="10.0 0 0" rpy="0 0 0" />
	</joint>
	
	<link name="EE">
	  <visual>
	    <geometry>
	      <sphere radius="0.2"/>
	    </geometry>
	    <origin rpy="5 0 0" xyz="0 0 0" /> <!-- rpy="5 0 0" -->
	    <material name="blue"/>
	  </visual>
	</link>


</robot>
